{
  "expo": {
    "name": "Personal Life Tracker",
    "slug": "personal-life-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "personal-life-tracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": [
      "ios"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.PersonalLifeTracker.prod",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleDisplayName": "Personal Life Tracker",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library so you can select images for your personal life tracking.",
        "NSHealthShareUsageDescription": "This app reads your step count to display your activity progress.",
        "NSHealthUpdateUsageDescription": "This app writes your health data (steps, workouts) to HealthKit to track your personal progress.",
        "NSSupportsLiveActivities": true
      }
    },
    "plugins": [
      "expo-router",
      ["expo-live-activity", { "bundleIdentifier": "com.PersonalLifeTracker.prod" }]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "106c3891-e834-4405-ab18-f14ae8eeeaf3"
      }
    },
    "android": {
      "package": "com.PersonalLifeTracker.prod"
    }
  }
}
